{
  "name": "new-react-app",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "@reach/router": "^1.2.1",
    "classnames": "^2.2.6",
    "croods-light": "^0.1.0",
    "croods-light-auth": "^0.0.4",
    "lodash": "^4.17.11",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-scripts": "2.0.5",
    "seasoned-components": "^0.1.0"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint --max-warnings 0 .",
    "start": "NODE_PATH=src/ react-scripts start",
    "build": "NODE_PATH=src/ react-scripts build",
    "test": "NODE_PATH=src/ react-scripts test --env=jsdom --no-cache",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "5.6.0",
    "eslint-config-seasoned": "0.0.5",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "react-test-renderer": "^16.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
